<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="staffim_timestampable.event.timestampable_subscriber.class">Staffim\TimestampableBundle\Event\TimestampableSubscriber</parameter>
    </parameters>

    <services>
        <service id="staffim_rollbar.rollbar_listener" class="%staffim_timestampable.event.timestampable_subscriber.class%">
            <tag name="doctrine_mongodb.odm.event_subscriber" />
        </service>
    </services>
</container>
